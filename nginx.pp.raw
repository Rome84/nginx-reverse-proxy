node "localhost" {
file { 'nginx.pp':
    ensure => "file",
    owner  => "root",
    group  => "root",
    mode   => "644",
    content => "include nginx \n
                nginx::resource::server { 'localhost': \n
                listen_port => 80, \n
                proxy       => 'http://localhost:5601', \n
                } \n
    ",}
} 
